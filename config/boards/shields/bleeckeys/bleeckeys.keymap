            symbol_right_layer {
                display-name = "SYM RIGHT";
            bindings = <
&trans	&trans	&trans	&trans						&trans	&trans	&trans	&trans
&trans	&kp SLASH	&kp NUMBER_7	&kp NUMBER_8	&kp NUMBER_9	&kp MINUS		&trans	&trans	&trans	&trans	&trans	&trans
&trans	&kp BACKSLASH	&kp NUMBER_4	&kp NUMBER_5	&kp NUMBER_6	&kp EQUAL		&kp LEFT_PARENTHESIS	&kp RIGHT_PARENTHESIS	&kp LEFT_BRACKET	&kp RIGHT_BRACKET	&kp SEMI	&trans
&trans	&kp BSPC	&kp NUMBER_1	&kp NUMBER_2	&kp NUMBER_3	&kp NUMBER_0		&trans	&trans	&kp LEFT_BRACE	&kp RIGHT_BRACE	&kp SINGLE_QUOTE	&trans
		&kp COMMA	&trans	&kp DOT	&trans			&kp DOT	&trans	&kp COMMA		
	&trans	&trans	&trans	&trans	&trans			&trans	&trans	&trans	&trans	&trans
