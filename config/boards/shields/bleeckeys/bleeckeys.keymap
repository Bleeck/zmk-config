#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/ext_power.h>
#include <dt-bindings/zmk/pointing.h>
#include <zephyr/dt-bindings/input/input-event-codes.h>
#include <input/processors.dtsi>    
#include <dt-bindings/zmk/outputs.h>

#define QUICK_TAP_MS 125
#define TAPPING_TERM_MS 150
#define SLOW_TAPPING_TERM_MS 350
#define IDLE_TIMEOUT_MS 5000
#define REQ_IDLE_MS 350

#define KEYS_L 0 1 2 3 4 10 11 12 13 14 20 21 22 23 24 31 32 33
#define KEYS_R 5 6 7 8 9 15 16 17 18 19 25 26 27 28 29 36 37 38
#define KEYS_T 34 35 40 41 42 43 44 45 46 47 48



/ {

    macros {
        lm: lm {
            compatible = "zmk,behavior-macro-two-param";
            wait-ms = <0>;
            tap-ms = <0>;
            #binding-cells = <2>;
            bindings
                = <&macro_param_1to1>
                , <&macro_press &mo MACRO_PLACEHOLDER>
                , <&macro_param_2to1>
                , <&macro_press &kp MACRO_PLACEHOLDER>
                , <&macro_pause_for_release>
                , <&macro_param_2to1>
                , <&macro_release &kp MACRO_PLACEHOLDER>
                , <&macro_param_1to1>
                , <&macro_release &mo MACRO_PLACEHOLDER>
                ;
        };
    base_layer_macro: base_layer_macro {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        wait-ms = <30>;
        tap-ms = <40>;
        bindings = <&kp F23 &to 0>;
        };
    nav_layer_macro: nav_layer_macro {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        wait-ms = <30>;
        tap-ms = <40>;
        bindings = <&kp F24 &to 2>;
        };
    nav_arrows_layer_macro: nav_arrows_layer_macro {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        wait-ms = <30>;
        tap-ms = <40>;
        bindings = <&kp F24 &to 3>;
        };
    };

    combos {
        compatible = "zmk,combos";
        combo_capsmod_right {
            timeout-ms = <200>;
            key-positions = <56 59>;
            bindings = <&mo 1>;
        };
        combo_invertKeys_left {
            timeout-ms = <200>;
            key-positions = <52 54>;
            bindings = <&mo 9>;
        };
        combo_invertCapsMod{
            timeout-ms = <200>;
            key-positions = <52 55>;
            bindings = <&mo 10>;
        };
        combo_invertKeys_right {
            timeout-ms = <200>;
            key-positions = <57 59>;
            bindings = <&mo 9>;
        };
        combo_bluetooth{
            timeout-ms=<200>;
            key-positions = <1 2 3>;
            bindings = <&tog 8>;
        };
        combo_bluetooth_right{
            timeout-ms=<200>;
            key-positions = <4 5 6>;
            bindings = <&tog 8>;
        };
    };
    keymap {
        compatible = "zmk,keymap";
            default_layer {
                display-name = "BASE";
            bindings = <
&kp ESC	&base_layer_macro	&nav_layer_macro	&nav_arrows_layer_macro						&mkp MB1	&kp CAPSLOCK	&kp BSPC	&kp DEL
&kp TAB	&kp Q	&kp W	&kp E	&kp R	&kp T		&kp Y	&kp U	&kp I	&kp O	&kp P	&kp TAB
&kp LSHIFT	&kp A	&kp S	&kp D	&kp F	&kp G		&kp H	&kp J	&kp K	&kp L	&kp SEMI	&kp RSHIFT
&kp LCTRL	&kp Z	&kp X	&kp C	&kp V	&kp B		&kp N	&kp M	&kp COMMA	&kp DOT	&kp SINGLE_QUOTE	&kp RCTRL
		&kp LGUI	&kp LALT	&kp RET	&kp C_MUTE			&kp RET	&kp RALT	&kp RGUI		
	&mo 6	&mo 4	&mo 7	&mo 5	&mo 1			&kp SPACE	&mo 5	&mo 7	&mo 4	&mo 6
            >;

                sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
            };

            capsmod_layer {
                display-name = "CAPS MOD";
            bindings = <
&trans	&trans	&trans	&trans						&trans	&trans	&trans	&trans
&trans	&kp GRAVE	&kp LC(W)	&kp LC(X)	&kp LC(C)	&kp LC(V)		&kp PAGE_UP	&kp HOME	&kp UP_ARROW	&kp END	&trans	&trans
&trans	&kp LALT	&kp LSHIFT	&kp LCTRL	&kp BSPC	&kp DEL		&kp PAGE_DOWN	&kp LEFT_ARROW	&kp DOWN_ARROW	&kp RIGHT_ARROW	&trans	&trans
&trans	&kp LC(Z)	&mkp MB1	&mkp MB3	&mkp MB2	&kp SPACE		&kp BSPC	&kp DEL	&kp LESS_THAN	&kp GREATER_THAN	&trans	&trans
		&trans	&trans	&trans	&trans			&trans	&trans	&trans		
	&trans	&trans	&trans	&trans	&trans			&trans	&trans	&trans	&trans	&trans
            >;

                sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
            };

            navigation_layer {
                display-name = "NAV";
            bindings = <
&trans	&trans	&trans	&trans						&trans	&trans	&trans	&trans
&trans	&trans	&trans	&trans	&trans	&trans		&trans	&trans	&trans	&trans	&trans	&trans
&trans	&trans	&trans	&trans	&trans	&trans		&trans	&trans	&trans	&trans	&trans	&trans
&trans	&trans	&trans	&trans	&trans	&trans		&trans	&trans	&trans	&trans	&trans	&trans
